{
  "name": "cuka",
  "version": "1.0.6",
  "description": "Cuka web app framework",
  "main": "dist/cuka.js",
  "exports": {
    ".": {
      "require": "./dist/cuka.js",
      "import": "./dist/cuka.mjs",
      "types": "./lib/cuka.d.ts"
    }
  },
  "types": "lib/cuka.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.mjs",
    "test:cjs": "node test/app.js",
    "test:esm": "node test/app.mjs"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "keywords": ["cuka", "http", "framework", "middleware"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "rollup": "^4.19.1"
  },
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liliangCS/cuka"
  }
}
